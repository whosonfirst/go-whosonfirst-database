CREATE TABLE {{ .Name }} (
    id BIGINT(20) UNSIGNED NOT NULL,
    min_x DOUBLE,
    max_x DOUBLE,
    min_y DOUBLE,
    max_y DOUBLE,
    wof_id INTEGER,
    is_alt TINYINT,
    alt_label TEXT,
    geometry BLOB,
    lastmodified INTEGER,
    PRIMARY KEY (id)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4;

CREATE SPATIAL INDEX {{ .Name }}_spatial ON {{ .Name }} (min_x, max_x, min_y, max_y);